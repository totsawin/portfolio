---
export interface Props {
  title?: string;
}
const { title = `Wind's PortFolio` } = Astro.props as Props;
---

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
  </head>